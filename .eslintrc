{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "rules": {
    "comma-dangle": ["error", "always-multiline"],
    "react/forbid-prop-types": 0,
    "react/no-unescaped-entities": ["error", {"forbid": [">", "}"]}],
    "semi": [2, "never"],
    "react/prop-types": 0 // TEMPORARY
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react",
  ],
  "env": {
    "browser": true
  },
}